    <!-- Parent Concept: AI Systems -->
    <skos:Concept rdf:about="https://github.com/sindoc/knowyourai-framework/blob/main/ai-human-relationships.rdf#ai-systems">
        <skos:prefLabel>AI Systems</skos:prefLabel>
        <dc:description>An overarching concept for all AI categories</dc:description>
    </skos:Concept>

    <!-- Concept: AI as a Tool -->
    <skos:Concept rdf:about="https://github.com/sindoc/knowyourai-framework/blob/main/ai-human-relationships.rdf#ai-as-a-tool">
        <skos:prefLabel>AI as a Tool</skos:prefLabel>
        <skos:altLabel>Human-Controlled AI</skos:altLabel>
        <skos:broader rdf:resource="https://github.com/sindoc/knowyourai-framework/blob/main/ai-human-relationships.rdf#ai-systems"/>
        <dc:description>AI acts purely as a tool that is fully controlled by humans, enhancing human capabilities by automating routine tasks or providing analytical insights.</dc:description>
        <skos:example>AI-powered data analytics tools, Predictive text or grammar checkers, Image enhancement software</skos:example>
        <knowyourai:humanRole rdf:resource="https://github.com/sindoc/knowyourai-framework/blob/main/ontology.owl#decision-maker-supervisor"/>
        <knowyourai:humanRoleLabel>Decision-maker, supervisor</knowyourai:humanRoleLabel>
        <knowyourai:aiRole rdf:resource="https://github.com/sindoc/knowyourai-framework/blob/main/ontology.owl#supporting-tool"/>
        <knowyourai:aiRoleLabel>Supporting tool, augmenting human tasks</knowyourai:aiRoleLabel>
        <skos:note>Humans retain full control over the process, AI assists by offering speed and efficiency, AI operates under strict parameters set by humans</skos:note>

        <!-- Risk Profile -->
        <knowyourai:RiskProfile>
            <knowyourai:likelihood>Low</knowyourai:likelihood>
            <knowyourai:impact>Low</knowyourai:impact>
        </knowyourai:RiskProfile>

        <!-- Resources -->
        <skos:related rdf:resource="https://github.com/sindoc/knowyourai-framework/blob/main/ai-human-relationships.rdf#ai-as-a-tool/resources"/>
    
        <rdf:Description rdf:about="https://github.com/sindoc/knowyourai-framework/blob/main/ai-human-relationships.rdf#ai-as-a-tool/ai_for_everyone_by_andrew_ng.rdf">
