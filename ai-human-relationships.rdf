            <dc:title>AI for Everyone by Andrew Ng</dc:title>
            <foaf:isPrimaryTopicOf rdf:resource="https://www.coursera.org/learn/ai-for-everyone"/>
        </rdf:Description>

        <rdf:Description rdf:about="https://github.com/sindoc/knowyourai-framework/blob/main/ai-human-relationships.rdf#ai-as-a-tool/machine_learning_for_beginners.rdf">
            <dcterms:type>Course</dcterms:type>
            <dc:title>Machine Learning for Beginners</dc:title>
            <foaf:isPrimaryTopicOf rdf:resource="https://learn.microsoft.com/en-us/training/paths/ml-for-beginners/"/>
        </rdf:Description>
        
