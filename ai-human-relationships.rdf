    <!-- Concept: AI as a Manager -->
    <skos:Concept rdf:about="https://github.com/sindoc/knowyourai-framework/blob/main/ai-human-relationships.rdf#ai-as-a-manager">
        <skos:prefLabel>AI as a Manager</skos:prefLabel>
        <skos:altLabel>AI-Led Decision Making</skos:altLabel>
        <skos:broader rdf:resource="https://github.com/sindoc/knowyourai-framework/blob/main/ai-human-relationships.rdf#ai-systems"/>
        <dc:description>AI takes on more responsibility by making decisions within defined areas, with limited human oversight.</dc:description>
        <skos:example>Autonomous vehicles, Algorithmic trading in finance, AI-driven supply chain management</skos:example>
        <knowyourai:humanRole rdf:resource="https://github.com/sindoc/knowyourai-framework/blob/main/ontology.owl#decision-maker-supervisor"/>
        <knowyourai:humanRoleLabel>Supervisor, parameter setter</knowyourai:humanRoleLabel>
        <knowyourai:aiRole rdf:resource="https://github.com/sindoc/knowyourai-framework/blob/main/ontology.owl#supporting-tool"/>
        <knowyourai:aiRoleLabel>Manager, decision-maker within constraints</knowyourai:aiRoleLabel>
        <skos:note>AI operates with a high degree of autonomy, Humans set parameters but rely on AI for execution</skos:note>

        <!-- Risk Profile -->
        <knowyourai:RiskProfile>
            <knowyourai:likelihood>High</knowyourai:likelihood>
            <knowyourai:impact>High</knowyourai:impact>
        </knowyourai:RiskProfile>

        <!-- Resources -->
        <skos:related rdf:resource="https://github.com/sindoc/knowyourai-framework/blob/main/ai-human-relationships.rdf#ai-as-a-manager/resources"/>
    
