        </rdf:Description>

        <rdf:Description rdf:about="https://github.com/sindoc/knowyourai-framework/blob/main/ai-human-relationships.rdf#ai-as-an-assistant/human_+_machine:_reimagining_work_in_the_age_of_ai.rdf">
            <dcterms:type>Book</dcterms:type>
            <dc:title>Human + Machine: Reimagining Work in the Age of AI</dc:title>
            <foaf:isPrimaryTopicOf rdf:resource="https://www.amazon.com/dp/1633693864"/>
        </rdf:Description>

        <rdf:Description rdf:about="https://github.com/sindoc/knowyourai-framework/blob/main/ai-human-relationships.rdf#ai-as-an-assistant/ibm_watson_assistant.rdf">
