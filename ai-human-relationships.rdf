    <!-- Concept: AI as an Autonomous Agent -->
    <skos:Concept rdf:about="https://github.com/sindoc/knowyourai-framework/blob/main/ai-human-relationships.rdf#ai-as-an-autonomous-agent">
        <skos:prefLabel>AI as an Autonomous Agent</skos:prefLabel>
        <skos:altLabel>AI Independence</skos:altLabel>
        <skos:broader rdf:resource="https://github.com/sindoc/knowyourai-framework/blob/main/ai-human-relationships.rdf#ai-systems"/>
        <dc:description>AI systems function with full autonomy, requiring little to no human intervention.</dc:description>
        <skos:example>AI in military drones, Fully autonomous research systems, Autonomous self-improving AI</skos:example>
        <knowyourai:humanRole rdf:resource="https://github.com/sindoc/knowyourai-framework/blob/main/ontology.owl#decision-maker-supervisor"/>
        <knowyourai:humanRoleLabel>Observer, designer</knowyourai:humanRoleLabel>
        <knowyourai:aiRole rdf:resource="https://github.com/sindoc/knowyourai-framework/blob/main/ontology.owl#supporting-tool"/>
        <knowyourai:aiRoleLabel>Fully autonomous entity, decision-maker</knowyourai:aiRoleLabel>
        <skos:note>AI operates independently and adapts to new situations, Minimal to no human intervention after deployment</skos:note>

        <!-- Risk Profile -->
        <knowyourai:RiskProfile>
            <knowyourai:likelihood>High</knowyourai:likelihood>
            <knowyourai:impact>High</knowyourai:impact>
        </knowyourai:RiskProfile>

