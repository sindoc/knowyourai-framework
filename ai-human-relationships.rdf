    <!-- Concept: AI as an Augmenter -->
    <skos:Concept rdf:about="https://github.com/sindoc/knowyourai-framework/blob/main/ai-human-relationships.rdf#ai-as-an-augmenter">
        <skos:prefLabel>AI as an Augmenter</skos:prefLabel>
        <skos:altLabel>Human-AI Symbiosis</skos:altLabel>
        <skos:broader rdf:resource="https://github.com/sindoc/knowyourai-framework/blob/main/ai-human-relationships.rdf#ai-systems"/>
        <dc:description>AI augments human capabilities, allowing humans to extend their cognitive, physical, or sensory abilities beyond natural limits.</dc:description>
        <skos:example>AI-enhanced creativity tools, Wearable AI devices, AI in personalized learning platforms</skos:example>
        <knowyourai:humanRole rdf:resource="https://github.com/sindoc/knowyourai-framework/blob/main/ontology.owl#decision-maker-supervisor"/>
        <knowyourai:humanRoleLabel>Co-creator, augmented operator</knowyourai:humanRoleLabel>
        <knowyourai:aiRole rdf:resource="https://github.com/sindoc/knowyourai-framework/blob/main/ontology.owl#supporting-tool"/>
        <knowyourai:aiRoleLabel>Cognitive or physical augmenter, enabler</knowyourai:aiRoleLabel>
        <skos:note>AI enhances human performance in real time, Seamless integration between human intuition and AI processing</skos:note>

        <!-- Risk Profile -->
        <knowyourai:RiskProfile>
            <knowyourai:likelihood>Medium</knowyourai:likelihood>
            <knowyourai:impact>High</knowyourai:impact>
        </knowyourai:RiskProfile>

        <!-- Resources -->
        <skos:related rdf:resource="https://github.com/sindoc/knowyourai-framework/blob/main/ai-human-relationships.rdf#ai-as-an-augmenter/resources"/>
    
