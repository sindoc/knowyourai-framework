    <!-- Concept: AI as an Assistant -->
    <skos:Concept rdf:about="https://github.com/sindoc/knowyourai-framework/blob/main/ai-human-relationships.rdf#ai-as-an-assistant">
        <skos:prefLabel>AI as an Assistant</skos:prefLabel>
        <skos:altLabel>Collaborative AI</skos:altLabel>
        <skos:broader rdf:resource="https://github.com/sindoc/knowyourai-framework/blob/main/ai-human-relationships.rdf#ai-systems"/>
        <dc:description>AI works alongside humans in a collaborative capacity, making recommendations and helping to solve problems. Humans retain the final decision-making authority.</dc:description>
        <skos:example>Virtual assistants (e.g., Siri, Alexa), AI in customer service (chatbots), Decision support systems</skos:example>
        <knowyourai:humanRole rdf:resource="https://github.com/sindoc/knowyourai-framework/blob/main/ontology.owl#decision-maker-supervisor"/>
        <knowyourai:humanRoleLabel>Partner, guide, decision-maker</knowyourai:humanRoleLabel>
        <knowyourai:aiRole rdf:resource="https://github.com/sindoc/knowyourai-framework/blob/main/ontology.owl#supporting-tool"/>
        <knowyourai:aiRoleLabel>Assistant, advisor, providing inputs and recommendations</knowyourai:aiRoleLabel>
        <skos:note>Collaborative interaction, AI learns from human feedback, Humans and AI co-create solutions</skos:note>

        <!-- Risk Profile -->
        <knowyourai:RiskProfile>
            <knowyourai:likelihood>Medium</knowyourai:likelihood>
            <knowyourai:impact>Medium</knowyourai:impact>
        </knowyourai:RiskProfile>

        <!-- Resources -->
        <skos:related rdf:resource="https://github.com/sindoc/knowyourai-framework/blob/main/ai-human-relationships.rdf#ai-as-an-assistant/resources"/>
    
        <rdf:Description rdf:about="https://github.com/sindoc/knowyourai-framework/blob/main/ai-human-relationships.rdf#ai-as-an-assistant/ai_and_robotics:_collaborative_systems.rdf">
